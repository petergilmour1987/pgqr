{
  "name": "pgqr",
  "version": "1.0.9",
  "keywords": [
    "qrcode",
    "qr",
    "svg",
    "logo",
    "vector"
  ],
  "author": "petergilmour1987@gmail.com",
  "license": "MIT",
  "description": "Simple QR Code generator with svg logo support",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/petergilmour1987/pgqr.git"
  },
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./react": "./dist/react/index.js"
