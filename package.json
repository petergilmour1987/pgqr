    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "husky": "^9.0.11",
    "@biomejs/biome": "^1.6.3",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "typescript": "^5.4.3"
